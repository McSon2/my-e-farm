  var largeur=740;
  var hauteur=630;
  var top=(screen.height-hauteur)/2;
  var left=(screen.width-largeur)/2;
  t=window.open('visite.php', 'myefarmvisite','top='+top+',left='+left+',toolbar=no,status=no,scrollbars=no,width='+largeur+',height='+hauteur);
  t.focus();
}







