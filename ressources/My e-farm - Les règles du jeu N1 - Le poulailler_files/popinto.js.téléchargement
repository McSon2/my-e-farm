	hideMenu();

	// On rempli le formulaire
	//document.poule.action='vente_animaux.php';
	// On prepare les parametres du get
	//param='type=1005&nbm='+document.poule.coq.value+'&nbf='+document.poule.poule.value;

	// On genere le menu
	str=getMenuHeader();
	// Le contenu du menu
	str+='<tr><td><nobr><img src="https://web.archive.org/web/20071013104955/http://img.myefarm.com/img/puce.gif" align=left><a href="javascript:validVenteN2Poule(0);">Coopérative</a><img src="https://web.archive.org/web/20071013104955/http://img.myefarm.com/img/px.gif" width=8></nobr></td></tr>';
